    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "imagekit": "^4.1.3",
    "imagekit-javascript": "^1.5.5",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.2",
    "multer": "^1.4.5-lts.1",
    "parse-duration": "^1.1.0",
    "prettier": "^3.0.0",
