{
  "name": "mapnews-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "tsstart": "nodemon ./src/server.ts",
    "start": "node dist/server.js",
    "tsc": "tsc",
    "build": "tsc",
    "swarm-init": "docker swarm init",
    "start-deployment": "docker build -t mapnews-notification ./notification && docker build -t mapnews-backend:latest . &&  docker stack deploy --compose-file docker-compose.yml mapnews-backend",
    "stop-deployment": "docker stack rm mapnews-backend",
    "restart-deployment": "npm run stop-deployment && docker stack deploy --compose-file docker-compose.yml mapnews-backend",
    "test-swarm": "docker stack deploy --compose-file docker-compose.test.yml mapnews",
    "format": "npx prettier --write ."
  },
  "author": "",
  "license": "ISC",
